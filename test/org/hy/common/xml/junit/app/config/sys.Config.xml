			</item>

			<item id="BarcodeGunServerPort">
				<name>BarcodeGunServerPort</name>
				<value>8081</value>
				<comment>扫码枪服务接口的IP地址</comment>
			</item>
	        
	        <item id="SYSID">
	            <name>SYSID</name>
				<value>XXApp</value>
